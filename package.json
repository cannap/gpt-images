		"eslint-plugin-svelte": "^2.35.1",
		"gh-pages": "^6.1.0",
		"postcss": "^8.4.24",
		"postcss-load-config": "^4.0.1",
		"prettier": "^3.1.0",
		"prettier-plugin-tailwindcss": "^0.4.1",
